{
    "name": "Tabsquash",
    "description": "Merges tabs with same web adderesses",
    "version": "0.0.3",
    "manifest_version": 3,
    "background":{
        "service_worker" : "background.js"
    },
    "permissions":[
        "tabs","activeTab"
    ],
    "action":{
        "default_popup": "popup.html",
        "default_icon": {
            "16": "images/sqadoosh.png",
            "32": "images/sqadoosh32.png"
        }
    },
    "icons":{
        "16": "images/sqadoosh.png",
        "32": "images/sqadoosh32.png"
       
    }


  }